      p2    =  intercambioArreglo;
              for(iSeg=0; iSeg<NUMSEGMENTOS; iSeg++){

                        p1 =datosAuxiliar + tamanioSegmento*segmentos[iSeg];
			p2  += tamanioSegmento;
   
			memcpy(p2,p1, tamanioSegmento);
		   
		}/*Fin del for para la permutacion de segmentos*/
                
                

