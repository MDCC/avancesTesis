library(ggplot2)
#indicamos la ruta donde se encuentra el archivo.
setwd("/home/user/Dropbox/mis documentos/MateriasCimatTomadas/4toSemestre/tesisproyecto/pruebasLogistico/esquema5mio")
frecuencia<-read.table("graficar.dat")
plot( frecuencia$V1, frecuencia$V2, type = "h",  lwd=14.7,  lend=2, xlab = "Valor", col="black",   ylab = "Frecuencia",  ylim=c(0,8000))


library(ggplot2)
#indicamos la ruta donde se encuentra el archivo.
setwd("/home/user/Dropbox/mis documentos/MateriasCimatTomadas/4toSemestre/tesisproyecto/pruebasLogistico/esquema5mio")
frecuencia<-read.table("graficarEnt.dat")
plot( frecuencia$V1, frecuencia$V2,  type = "h",  lwd=14.7,  lend=2, col="black",  xlab = "Valor", ylab = "Frecuencia",  ylim=c(0,2000))



qplot(frecuencia$V2, geom = "histogram", binwidth = 1, xlim = c(0,1))

