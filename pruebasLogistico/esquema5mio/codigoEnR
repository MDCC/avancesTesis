


library(ggplot2)
#indicamos la ruta donde se encuentra el archivo.
setwd("/home/user/Dropbox/mis documentos/MateriasCimatTomadas/4toSemestre/tesisproyecto/pruebasLogistico/esquema1")
frecuencia<-read.table("graficar.dat")
plot( frecuencia$V1, frecuencia$V2, type = "h", xlab = "Particiones", ylab = "Frecuencia",  ylim=c(0,6000), 
      main = "Frecuencia (Esquema 1)")


